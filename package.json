    "lint": "yarn check-node-version && eslint 'src/**/*.ts' 'tests/**/*.ts' --cache",
    "start": "yarn build && node dist/main.js",
    "check-node-version": "check-node-version --package"
  },
  "devDependencies": {
    "@tlan16/eslint-config-typescript": "^0.0.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
