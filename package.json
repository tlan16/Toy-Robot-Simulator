{
  "name": "@tlan16/toy-robot-simulator",
  "version": "1.0.0",
  "main": "src/main.ts",
  "repository": "git@github.com:tlan16/Toy-Robot-Simulator.git",
  "author": "Frank Lan <flan@msts.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "build": "yarn check-node-version && tsc -p tsconfig.build.json",
