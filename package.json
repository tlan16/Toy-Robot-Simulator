    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14.15.1",
    "npm": ">=6.14.9",
    "yarn": ">=1.22.10"
  }
